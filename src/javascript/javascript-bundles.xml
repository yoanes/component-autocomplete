<?xml version="1.0" encoding="UTF-8"?>
<bs:bundlesConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:bs="http://mobile.sensis.com.au/build-scripts"
    xsi:schemaLocation="http://mobile.sensis.com.au/build-scripts au.com.sensis.mobile.bundles.xsd">
    
    <bundles basename="autocomplete-component" subpackage="autocomplete">
        <bundle name="common" abstract="true">
            <fileset dir="${src.javascript.dir}/autocomplete/device-invariant">
            </fileset>
        </bundle>
        <bundle name="default" parent="common">
            <fileset dir="${src.javascript.dir}/autocomplete/device-variant/default">
            </fileset>
        </bundle>
         <bundle name="android" parent="common">
            <fileset dir="${src.javascript.dir}/autocomplete/device-variant/android-os">
            </fileset>
        </bundle>
    </bundles>
  
</bs:bundlesConfig>
